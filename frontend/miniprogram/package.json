{
    "name": "miniprogram",
    "version": "1.0.0",
    "private": true,
    "description": "WeChat Miniprogram using Taro",
    "templateInfo": {
        "name": "default",
        "typescript": true,
        "css": "sass"
    },
    "scripts": {
        "build:weapp": "taro build --type weapp",
        "build:weapp:watch": "npm run build:weapp -- --watch",
        "dev:weapp": "npm run build:weapp -- --watch",
        "build:h5": "taro build --type h5",
        "dev:h5": "npm run build:h5 -- --watch",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage"
    },
    "browserslist": [
        "last 3 versions",
        "Android >= 4.1",
        "ios >= 8"
    ],
    "dependencies": {
        "@babel/runtime": "^7.24.0",
        "@tarojs/components": "4.0.7",
        "@tarojs/helper": "4.0.7",
        "@tarojs/plugin-framework-react": "4.0.7",
        "@tarojs/plugin-platform-weapp": "4.0.7",
        "@tarojs/react": "4.0.7",
        "@tarojs/runtime": "4.0.7",
        "@tarojs/shared": "4.0.7",
        "@tarojs/taro": "4.0.7",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@babel/core": "^7.24.0",
        "@babel/preset-react": "^7.24.0",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
        "@tarojs/cli": "4.0.7",
        "@tarojs/taro-loader": "4.0.7",
        "@tarojs/webpack5-runner": "4.0.7",
        "@types/node": "^22.10.1",
        "@types/react": "^18.3.18",
        "@types/webpack-env": "^1.18.5",
        "@vitest/ui": "^2.1.0",
        "@testing-library/react": "^16.1.0",
        "babel-preset-taro": "4.0.7",
        "eslint": "^9.18.0",
        "eslint-config-taro": "4.0.7",
        "postcss": "^8.5.6",
        "react-refresh": "^0.14.2",
        "sass": "^1.83.4",
        "typescript": "~5.6.3",
        "vitest": "^2.1.0",
        "webpack": "^5.98.0"
    }
}